<h1>Update Merchant</h1>
<%= form_tag "/merchants/#{@merchant.id}", method: :patch do %>
 <%= label_tag :name %>
 <%= text_field_tag :name %>
 <%= label_tag :address %>
 <%= text_field_tag :address %>
 <%= label_tag :city %>
 <%= text_field_tag :city %>
 <%= label_tag :state %>
 <%= text_field_tag :state %>
 <%= label_tag :zip %>
 <%= text_field_tag :zip %>

 <%= submit_tag 'Update Merchant' %>
<% end  %>
